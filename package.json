{
  "name": "clubedoshoteis-web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.13.1",
    "npm": ">=10.8.0"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "testRegex": "./tests/.*\\.test\\.ts$",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --detectOpenHandles"
